PATH=$PATH:$JAVA_HOME/bin 
export PATH 
CLASSPATH=$CLASSPATH:$JAVA_HOME/lib/tools.jar:$JAVA_HOME/lib/mysql-connector-java-5.1.45.jar 
export CLASSPATH


